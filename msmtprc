defaults
logfile ~/.msmtp.log
auth on
tls on

dsn_notify failure,delay
dsn_return off

account wormnet
host %digriz.smtp%
port 587
tls_fingerprint 4E:0F:55:70:86:29:0F:5A:29:A5:2F:D0:68:FD:6F:F6:5B:C7:8F:03
user %digriz.email%
password %digriz.password%
dsn_notify off

account telemetry
host %telemetry.smtp%
port 587
tls_trust_file /etc/ssl/certs/ca-certificates.crt
user %telemetry.email%
password %telemetry.password%

account default : wormnet
